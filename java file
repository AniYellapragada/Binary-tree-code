class Node
{
    int val;
    Node left, right;

    Node(int val) {
        this.val = val;
        left = null;
        right = null;
    }

}
public class Binary tree{
public static main(String[] args) {
  //create a bianary tree
    Node root = new Node(1);
    root.left = new Node(2);
    root.right = new Node(3);
    root.left.left = new Node(4);
    root.left.right = new Node(5);
    root.right.left = new Node(6);



}

public static void printTree(Node node) {
    if (node == null) {
        return;
    }
    System.out.print(node.val + " ");
    printTree(node.left);
    printTree(node.right);
}
public static void Binarytreesearch(Node node, int number){
    if( node ==null){
        System.out.println("Number not found: " + number);
        return;
    }
    if( number == node.val)
    {
        System.out.println("Number found: " + number);
        return;
    }
    if (number < node.val) {
        Binarytreesearch(node.left, number);
    } else {
        Binarytreesearch(node.right, number);
    }
}
